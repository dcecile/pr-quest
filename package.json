{
  "private": true,
  "scripts": {
    "dev": "eslint webpack.config.babel.js && cross-env NODE_ENV=development webpack-dev-server --progress --hot",
    "build": "yarn run export-data && eslint webpack.config.babel.js && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "update-data": "eslint data/update.js && babel-node data/update.js",
    "export-data": "eslint data/export-objects.js && babel-node data/export-objects.js"
  },
  "devDependencies": {
    "airtable": "^0.4.5",
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "extract-text-webpack-plugin": "@^2.0.0-beta",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "normalize-styl": "^4.1.1",
    "parse5": "^3.0.2",
    "path-to-regexp": "^1.7.0",
    "pug": "^2.0.0-beta11",
    "pug-loader": "^2.3.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "url-loader": "^0.5.7",
    "vue": "^2.1.0",
    "vue-loader": "^10.0.0",
    "vue-router": "^2.4.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-async-to-generator"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "vue"
    ]
  }
}
